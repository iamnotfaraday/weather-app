datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model weatherCurrentCache {
  city       String    @id @db.VarChar(100)
  data       Json
  expires_at BigInt
  updated_at DateTime? @default(now()) @db.Timestamp(0)

   @@map("weather_current_cache")
}

model weatherForecastCache {
  city       String    @id @db.VarChar(100)
  data       Json
  expires_at BigInt
  updated_at DateTime? @default(now()) @db.Timestamp(0)

  @@map("weather_forecast_cache")
}
